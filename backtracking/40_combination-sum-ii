class Solution:
    def combinationSum2(self, candidates: List[int], target: int) -> List[List[int]]:
        res=[]
        candidates.sort()
        n=len(candidates)
        def helper(s,path,i):
            if s>target or i>n:
                return
            if s==target:
                path.sort()
                if  path not in res:
                    res.append(path)
                return
            for j in range(i,n):
                if j>i and candidates[j]==candidates[j-1]:
                    continue
                helper(s+candidates[j],path+[candidates[j]],j+1)
        helper(0,[],0)
        return res
