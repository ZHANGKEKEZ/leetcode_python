class Solution:
    def simplifyPath(self, path: str) -> str:
        paths=path.split('/')
        res=[]
        for path in paths:
            if path=='..' and res:
                res.pop()
            #这里的不等于空很关键，遇到//时，split是把两个/之间当作""存储的
            if path!='.'and path!='..'and path!='':
                res.append(path)
        #注意字符串join函数的用法
        r="/"+"/".join(res)
        return r
